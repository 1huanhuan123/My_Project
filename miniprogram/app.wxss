/**app.wxss**/
/* 引入字体图标 */
@font-face {
  font-family: "iconfont";
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-landscape:before {
  content: "\e600";
}

.icon-food:before {
  content: "\e606";
}

.icon-travel:before {
  content: "\e609";
}

.icon-life:before {
  content: "\e60a";
}

.icon-work:before {
  content: "\e60b";
}

.icon-other:before {
  content: "\e60c";
}

.icon-view:before {
  content: "\e60d";
}

.icon-favorite:before {
  content: "\e60e";
}

.icon-edit:before {
  content: "\e60f";
}

.icon-delete:before {
  content: "\e610";
}

.icon-arrow:before {
  content: "\e611";
}

.icon-search:before {
  content: "\e612";
}

.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #f6f6f6;
  min-height: 100vh;
}

.page-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 30rpx;
}

.section {
  margin-bottom: 30rpx;
}

.section-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 20rpx;
}

.section-content {
  background-color: #fff;
  border-radius: 12rpx;
  padding: 20rpx;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.1);
}

.btn-primary {
  background-color: #4A90E2;
  color: #fff;
  border-radius: 40rpx;
  font-size: 30rpx;
  line-height: 1.5;
}

.btn-default {
  background-color: #f6f6f6;
  color: #666;
  border: 1rpx solid #ddd;
  border-radius: 40rpx;
  font-size: 30rpx;
  line-height: 1.5;
}

.form-group {
  margin-bottom: 20rpx;
}

.form-label {
  font-size: 28rpx;
  color: #666;
  margin-bottom: 10rpx;
}

.form-input {
  background-color: #f9f9f9;
  border-radius: 8rpx;
  padding: 20rpx;
  font-size: 28rpx;
  color: #333;
}

.form-textarea {
  background-color: #f9f9f9;
  border-radius: 8rpx;
  padding: 20rpx;
  font-size: 28rpx;
  color: #333;
  width: 100%;
  height: 200rpx;
}

.grid {
  display: flex;
  flex-wrap: wrap;
}

.grid-item {
  width: 33.33%;
  padding: 10rpx;
  box-sizing: border-box;
} 

.tag {
  display: inline-block;
  padding: 6rpx 16rpx;
  background-color: #f0f7ff;
  color: #4A90E2;
  border-radius: 30rpx;
  font-size: 24rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}